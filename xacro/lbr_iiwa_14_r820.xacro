<?xml version="1.0"?>
<robot name="lbr_iiwa_14_r820" xmlns:xacro="http://wiki.ros.org/xacro">

<!-- ****************************************************** Properties**************************************************************** -->

  <xacro:property name="mesh_path"        value="package://bots/meshes/lbr_iiwa_14_r820/"/>

<!-- ****************************************************** Includes *****************************************************************  --> 

  <xacro:include filename="$(find bots)/xacro/common_kuka_materials.xacro" />   <!-- RVIZ Colors                                -->
  <xacro:include filename="$(find bots)/gazebo/gz_lbr_iiwa_14_r820.xacro" />    <!-- Gazebo                                     -->

<!-- ****************************************************** Macro ******************************************************************** -->

  <xacro:macro name="kuka_lbr_iiwa_14_r820" params="parent prefix">

<!-- ****************************************************** LINKS ******************************************************************** -->

<!-- ****************************************************** base link **************************************************************** -->

    <link name="${prefix}base_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}visual/base_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}collision/base_link.stl" />
        </geometry>
      </collision>
    </link>

<!-- ****************************************************** link 1 (A1) ************************************************************** -->

    <link name="${prefix}link_1">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}visual/link_1.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}collision/link_1.stl" />
        </geometry>
      </collision>
    </link>

<!-- ****************************************************** link 2 (A2) *********************************************************** -->

    <link name="${prefix}link_2">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}visual/link_2.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}collision/link_2.stl" />
