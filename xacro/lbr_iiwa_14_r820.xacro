      </collision>
    </link>

<!-- ****************************************************** link 6 (A6) *********************************************************** -->

    <link name="${prefix}link_6">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}visual/link_6.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}collision/link_6.stl" />
        </geometry>
      </collision>
    </link>

<!-- ****************************************************** link 7 (A7) *********************************************************** -->

    <link name="${prefix}link_7">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}visual/link_7.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh_path}collision/link_7.stl" />
        </geometry>
      </collision>
    </link>

<!-- ****************************************************** tool 0 ******************************************************************* -->
<!-- ******************* Following REP199, this frame shall be use to attach EEF or other equipment ********************************** -->

  
    <link name="${prefix}tool0" />
<!-- ****************************************************** END LINKS **************************************************************** -->

<!-- ****************************************************** JOINTS ******************************************************************* -->
<!-- ****************************************************** Base Joint *************************************************************** -->

    <joint name="${prefix}base_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${parent}"/>
      <child link="${prefix}base_link" />
    </joint>

<!-- ****************************************************** joint 1 (A1) ************************************************************* -->

    <joint name="${prefix}joint_a1" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}link_1" />
      <axis xyz="0 0 1" />
      <limit lower="-2.9668" upper="2.9668" effort="0" velocity="1.4834" />
    </joint>

<!-- ****************************************************** joint 2 (A2) **************************************************************** -->


    <joint name="${prefix}joint_a2" type="revolute">
      <origin xyz="-0.00043624 0 0.36" rpy="0 0 0" />
      <parent link="${prefix}link_1" />
      <child link="${prefix}link_2" />
      <axis xyz="0 1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="0" velocity="1.4834" />
    </joint>

<!-- ****************************************************** joint 3 (A3) **************************************************************** -->

    <joint name="${prefix}joint_a3" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}link_2" />
      <child link="${prefix}link_3" />
      <axis xyz="0 0 1" />
      <limit lower="-2.9668" upper="2.9668" effort="0" velocity="1.7452" />
    </joint>

<!-- ****************************************************** joint 4 (A4) **************************************************************** -->
    
    <joint name="${prefix}joint_a4" type="revolute">
      <origin xyz="0.00043624 0 0.42" rpy="0 0 0" />
      <parent link="${prefix}link_3" />
      <child link="${prefix}link_4" />
      <axis xyz="0 -1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="0" velocity="1.3089" />
    </joint>

<!-- ****************************************************** joint 5 (A5) **************************************************************** -->

    <joint name="${prefix}joint_a5" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}link_4" />
      <child link="${prefix}link_5" />
      <axis xyz="0 0 1" />
      <limit lower="-2.9668" upper="2.9668" effort="0" velocity="2.2688" />
    </joint>

<!-- ****************************************************** joint 6 (A6) **************************************************************** -->

    <joint name="${prefix}joint_a6" type="revolute">
      <origin xyz="0 0 0.4" rpy="0 0 0" />
      <parent link="${prefix}link_5" />
      <child link="${prefix}link_6" />
      <axis xyz="0 1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="0" velocity="2.356" />
    </joint>

<!-- ****************************************************** joint 7 (A7) **************************************************************** -->

    <joint name="${prefix}joint_a7" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}link_6" />
      <child link="${prefix}link_7" />
      <axis xyz="0 0 1" />
      <limit lower="-3.0541" upper="3.0541" effort="0" velocity="2.356" />
    </joint>

<!-- ***************** This frame corresponds to the $TOOL coordinate system in KUKA KRC controllers ********************************* -->

    <joint name="${prefix}joint_a7-tool0" type="fixed">
      <origin xyz="0 0 0.126" rpy="0 0 0" />
      <parent link="${prefix}link_7" />
      <child link="${prefix}tool0" />
      <axis xyz="0 0 0" />
    </joint>


<!-- ****************************************************** END JOINTS *********************************************************** -->

<!-- ****************************************************** ROS base_link to KUKA $ROBROOT coordinate system transform *********** -->

    <link name="${prefix}base" />

    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

<!-- ****************************************************** End Macro **************************************************************** -->

  </xacro:macro>

<!-- ****************************************************** Generates a urdf from macro ********************************************** -->

  <xacro:kuka_lbr_iiwa_14_r820 parent="world" prefix=""/>

<!-- ****************************************************** End ********************************************************************** -->

</robot>

